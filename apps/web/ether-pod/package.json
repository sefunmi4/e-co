{
  "name": "@eco/web",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "copy-env": "node scripts/copy-env.js",
    "generate:config": "node scripts/generate-config.cjs",
    "sync-config": "npm --prefix ../../../packages/config run build && npm run generate:config",
    "pretest": "npm run copy-env && npm run sync-config",
    "dev": "npm run copy-env && npm run sync-config && next dev",
    "check:assets": "node scripts/check-textures.cjs",
    "build": "npm run copy-env && npm run sync-config && npm run check:assets && next build",
    "start": "npm run sync-config && next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@e-co/config": "file:../../../../packages/config",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-web": "^2.1.0",
    "@react-three/drei": "^9.86.5",
    "@react-three/fiber": "^8.14.5",
    "next": "^14.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplex-noise": "^4.0.3",
    "three": "^0.160.0",
    "toml": "^3.0.0",
    "zustand": "^5.0.8",
    "socket.io-client": "^4.7.5",
    "@vercel/og": "^0.6.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.3",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.1"
  }
}
